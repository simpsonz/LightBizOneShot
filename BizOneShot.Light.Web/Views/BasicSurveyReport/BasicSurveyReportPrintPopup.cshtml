@model BizOneShot.Light.Models.ViewModels.BasicSurveyPrintViewModel

@{
    Layout = null;
}

@Styles.Render("~/css/print")
<script>
    $(document).ready(function () {
        //top menu
        $('#naviwrap_biz > .navi_biz >  a.navi_report').addClass('on');
        //Cs menu
        $('#sub_leftwrap > .sub_gnbwrap > ul > li > a.gnb_basicReport').addClass('on');

        $('#mainForm input').attr('disabled', true);
        $('#mainForm textarea').attr('disabled', true);

        makeBarChart("Productivity", @Model.orgProductivity.Productivity.Company, @Model.orgProductivity.Productivity.AvgBizInCompany, @Model.orgProductivity.Productivity.AvgTotal, @Model.orgProductivity.Productivity.AvgSMCompany, "천원", 0);
        makeBarChart("Activity", @Model.orgProductivity.Activity.Company, @Model.orgProductivity.Activity.AvgBizInCompany, @Model.orgProductivity.Activity.AvgTotal, @Model.orgProductivity.Activity.AvgSMCompany, "%", 1);

        makeBarChart("Profitability", @Model.productivityProfitability.Profitability.Company, @Model.productivityProfitability.Profitability.AvgBizInCompany, @Model.productivityProfitability.Profitability.AvgTotal, @Model.productivityProfitability.Profitability.AvgSMCompany, "%", 1);
        makeBarChart("Growth", @Model.productivityProfitability.Growth.Company, @Model.productivityProfitability.Growth.AvgBizInCompany, @Model.productivityProfitability.Growth.AvgTotal, @Model.productivityProfitability.Growth.AvgSMCompany, "%", 1);
    });

    function makeBarChart(divName, company, avgBizInCompany, avgTotal, avgSMCompany, unit, point) {
        var param1 = company;
        var param2 = avgBizInCompany;
        var param3 = avgTotal;
        var param4 = avgSMCompany;

        Highcharts.setOptions({
            lang: {
                decimalPoint: '.',
                thousandsSep: ','
            }
        });


        chart = new Highcharts.Chart({
            chart: {
                type: 'column',
                renderTo: divName,
                margin: [10, 0, 25, 50],
                spacingTop: 0,
                //spacingBottom: 0,
                //spacingLeft: 0,
                spacingRight: 0
            },
            exporting: {                    // highcharts context menu 제거
                buttons: {
                    contextButtons: {
                        enabled: false,
                        menuItems: null
                    }
                },
                enabled: false
            },
            credits: {
                enabled: false
            },
            title: {
                style: {
                    display: 'none'
                }
            },
            xAxis: {
                type: 'category',
                gridLineWidth: 1
            },
            yAxis: {
                gridLineWidth: 0,
                title: {
                    style: {
                        display: 'none'
                    }
                },
                labels: {
                    enabled: true
                },
            },
            legend: {
                enabled:false
            },
            tooltip: { enabled: false },

            series: [{
                upColor: Highcharts.getOptions().colors[2],
                color: Highcharts.getOptions().colors[3],
                pointWidth: 35,
                enableMouseTracking: false,
                data: [{
                    name: "해당기업",
                    y: param1
                }, {
                    name: "참여기업평균",
                    y: param2
                }, {
                    name: "전체평균",
                    y: param3
                }, {
                    name: "중소기업 평균",
                    y: param4
                }],
                dataLabels: {
                    enabled: true,
                    formatter: function () {
                        return Highcharts.numberFormat(this.y, point, '.') + unit;
                    },
                    style: {
                        color: '#000000',
                        fontWeight: 'bold'//,
                        //textShadow: '0px 0px 3px black'
                    }
                },
                pointPadding: 0
            }]
        });
    }

    window.print();
</script>

